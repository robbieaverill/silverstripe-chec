{
    "name": "robbieaverill/silverstripe-chec",
    "description": "A Silverstripe 4 module which integrates with Chec",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "chec.io",
        "chec"
    ],
    "require": {
        "php": "^7.3",
        "silverstripe/cms": "^4.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "squizlabs/php_codesniffer": "^3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Robbie\\SilverstripeChec\\": "src/",
            "Robbie\\SilverstripeChec\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
